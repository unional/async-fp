    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-harmony": "^7.1.2",
    "husky": "^8.0.1",
    "pinst": "^3.0.0",
    "rimraf": "^5.0.0",
    "turbo": "^1.4.3",
    "typescript": "5.1.6"
