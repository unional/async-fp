{
  "name": "@unional/gizmo",
  "version": "1.2.0",
  "description": "Building gizmos",
  "homepage": "https://github.com/unional/async-fp/tree/main/packages/gizmo#readme",
  "bugs": {
    "url": "https://github.com/unional/async-fp/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/async-fp.git",
    "directory": "packages/gizmo"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "node": "./esm/index.js",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "node": "./cjs/index.js",
        "default": "./cjs/index.js"
      }
    },
    "./testing": {
      "import": {
        "types": "./esm/testing.d.ts",
        "node": "./esm/testing.js",
        "default": "./esm/testing.js"
      },
      "require": {
        "types": "./cjs/testing.d.ts",
        "node": "./cjs/testing.js",
        "default": "./cjs/testing.js"
      }
    }
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "jsnext:main": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "testing",
    "ts",
    "!**/*.{spec,unit,accept,system,perf,stress,test}.*"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json && ncp ../../.config/package.cjs.json ./cjs/package.json",
    "build:esm": "tsc",
    "clean": "rimraf cjs esm coverage *.tsbuildinfo",
    "coverage": "jest --coverage",
    "depcheck": "depcheck",
    "lint": "cross-env TIMING=1 eslint --ext=ts,js,cjs,mjs .",
    "nuke": "run-s clean nuke:local",
    "nuke:local": "rimraf node_modules",
    "prepublishOnly": "run-s build",
    "size": "size-limit",
    "test": "jest",
    "test:watch": "jest --watch",
    "w": "pnpm test:watch",
    "verify": "npm-run-all -p lint build coverage -p depcheck size"
  },
  "devDependencies": {
    "@repobuddy/jest": "^3.2.0",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "assertron": "^11.0.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-harmony": "^7.1.2",
    "jest": "^29.5.0",
    "jest-esm-transformer-2": "^1.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config-2": "^2.1.0",
    "jest-watch-typeahead": "^2.2.2",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "size-limit": "^8.0.1",
    "ts-jest": "^29.1.0",
    "type-plus": "^6.6.0",
    "typescript": "5.1.0-dev.20230430"
  },
  "engines": {
    "node": ">= 14.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "./cjs/index.js"
    },
    {
      "path": "./esm/index.js"
    }
  ]
}
