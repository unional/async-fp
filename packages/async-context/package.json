{
  "name": "@unional/async-context",
  "version": "9.0.10",
  "description": "Asynchronous context for functional style programming",
  "keywords": [
    "async",
    "context",
    "dependency",
    "functional",
    "typescript"
  ],
  "homepage": "https://github.com/unional/async-fp/tree/main/packages/async-context",
  "bugs": {
    "url": "https://github.com/unional/async-fp/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/async-fp.git",
    "directory": "packages/async-context"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js"
      }
    }
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "jsnext:main": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts",
    "!**/*.{spec,test,unit,accept,integrate,system}.*"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "buddy ts build cjs",
    "build:esm": "tsc",
    "clean": "rimraf cjs esm coverage *.tsbuildinfo",
    "coverage": "jest --coverage",
    "depcheck": "depcheck",
    "lint": "cross-env TIMING=1 eslint --ext=ts,js,cjs,mjs .",
    "nuke": "pnpm clean && rimraf node_modules",
    "prepublishOnly": "pnpm build",
    "size": "size-limit",
    "test": "jest",
    "verify": "npm-run-all -p lint build coverage -p depcheck size"
  },
  "dependencies": {
    "iso-error": "^6.0.0",
    "type-plus": "^6.6.0"
  },
  "devDependencies": {
    "@repobuddy/jest": "^3.2.0",
    "@repobuddy/typescript": "^1.2.0",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/setimmediate": "^1.0.2",
    "assertron": "^11.0.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "jest": "^29.5.0",
    "jest-esm-transformer-2": "^1.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config-2": "^2.1.0",
    "jest-watch-typeahead": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "repobuddy": "^1.0.2",
    "rimraf": "^5.0.0",
    "setimmediate": "^1.0.5",
    "size-limit": "^8.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "5.1.0-dev.20230506"
  },
  "engines": {
    "node": ">= 14.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "./cjs/index.js"
    },
    {
      "path": "./esm/index.js"
    }
  ]
}
