{
  "name": "async-fp",
  "version": "9.0.0",
  "description": "Utility library for asynchronous functional programming",
  "keywords": [
    "async",
    "context",
    "dependency",
    "functional",
    "typescript"
  ],
  "homepage": "https://github.com/unional/async-fp/tree/master/packages/async-fp",
  "bugs": {
    "url": "https://github.com/unional/async-fp/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/async-fp.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./cjs/index.d.ts",
        "default": "./cjs/index.js"
      }
    },
    "./gizmo": {
      "import": {
        "types": "./esm/gizmo.d.ts",
        "default": "./esm/gizmo.js"
      },
      "require": {
        "types": "./cjs/gizmo.d.ts",
        "default": "./cjs/gizmo.js"
      }
    }
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "jsnext:main": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts",
    "!**/*.{spec,test,unit,accept,integrate,system}.*"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json && ncp ../../.config/package.cjs.json ./cjs/package.json",
    "build:esm": "tsc",
    "clean": "rimraf cjs esm coverage *.tsbuildinfo",
    "coverage": "jest --coverage",
    "depcheck": "depcheck",
    "lint": "cross-env TIMING=1 eslint --ext=ts,js,cjs,mjs .",
    "nuke": "pnpm clean && rimraf node_modules",
    "prepublishOnly": "pnpm build",
    "size": "size-limit",
    "test": "jest",
    "test:watch": "jest --watch",
    "verify": "npm-run-all -p lint build coverage -p depcheck size"
  },
  "dependencies": {
    "@unional/async-context": "workspace:^",
    "@unional/gizmo": "workspace:^"
  },
  "devDependencies": {
    "@repobuddy/jest": "^3.1.3",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "assertron": "^11.0.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-harmony": "^7.1.2",
    "jest": "^29.4.3",
    "jest-esm-transformer-2": "^1.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config-2": "^2.1.0",
    "jest-watch-typeahead": "^2.2.2",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "size-limit": "^8.0.1",
    "ts-jest": "^29.0.0",
    "type-plus": "^6.4.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">= 14.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "./cjs/index.js"
    },
    {
      "path": "./esm/index.js"
    }
  ]
}
