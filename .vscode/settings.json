{
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.formatOnSave": true
	},
	"[javascript]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true
	},
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/*.code-search": true,
		"**/.tsbuildinfo": true
	},
	"cSpell.words": [
		"changesets",
		"depcheck",
		"jsnext",
		"lerna",
		"macos",
		"miku",
		"npmjs",
		"npmrc",
		"pinst",
		"repobuddy",
		"testname",
		"tsbuildinfo",
		"vitest",
		"whoami"
	],
	"typescript.tsdk": "node_modules/typescript/lib"
}
